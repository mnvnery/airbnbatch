<h1>here is where the form will be</h1>

<%= simple_form_for @personality do |f| %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :price_hour %>
  <%= f.input :image_url %>
  <%= f.button :submit, class: "btn-primary" %>
  <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
<% end %>

<div>
  <p></p>
  <%= link_to "Back", personalities_path %>
</div>

